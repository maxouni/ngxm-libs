{
  "name": "ngxm-libs",
  "version": "0.0.5",
  "scripts": {
    "ng": "ng",
    "start:demo": "ng serve demo",
    "build:demo": "ng build demo --configuration production --base-href https://maxouni.github.io/ngxm-libs/",
    "build:ngxm-input": "ng build ngxm-input --configuration production",
    "build:ngxm-textarea": "ng build ngxm-textarea --configuration production",
    "publish:ngxm-input": "npm run build:ngxm-input && cd dist/ngxm-input && npm pack && npm publish",
    "publish:ngxm-textarea": "npm run build:ngxm-textarea && cd dist/ngxm-textarea && npm pack && npm publish",
    "test": "ng test"
  },
  "authors": [
    "Max Mishin <max@mishin.io>"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "bootstrap": "^5.1.3",
    "normalize.css": "^8.0.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.2",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.0",
    "prettier": "^2.5.1",
    "typescript": "~4.5.2"
  }
}
